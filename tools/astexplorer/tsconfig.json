
{
  "compilerOptions": {
    "jsx": "react",
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es5",
      "es6"
    ]
  },
  "include": [
    "src/**/*"
  ]
}
